{
  "storage": {
    "moviesdb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "bookingsdb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {},
  "api": {},
  "auth": {
    "movieBookingApp": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false,
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [],
        "signupAttributes": [
          "PREFERRED_USERNAME",
          "NAME",
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 6,
          "passwordPolicyCharacters": []
        },
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      }
    }
  }
}